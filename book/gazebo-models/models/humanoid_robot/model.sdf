<?xml version="1.0" ?>
<sdf version="1.7">
  <model name="humanoid_robot">
    <static>false</static>
    <self_collide>false</self_collide>
    <enable_wind>false</enable_wind>
    <allow_auto_disable>true</allow_auto_disable>

    <!-- Base link -->
    <link name="base_link">
      <inertial>
        <mass>10.0</mass>
        <inertia>
          <ixx>0.4</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.4</iyy>
          <iyz>0.0</iyz>
          <izz>0.2</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.15</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.7 0.7 0.7 1</ambient>
          <diffuse>0.7 0.7 0.7 1</diffuse>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.15</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <!-- Head -->
    <link name="head">
      <inertial>
        <mass>2.0</mass>
        <inertia>
          <ixx>0.02</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.02</iyy>
          <iyz>0.0</iyz>
          <izz>0.02</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <sphere>
            <radius>0.1</radius>
          </sphere>
        </geometry>
        <material>
          <ambient>1.0 1.0 1.0 1</ambient>
          <diffuse>1.0 1.0 1.0 1</diffuse>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <sphere>
            <radius>0.1</radius>
          </sphere>
        </geometry>
      </collision>
    </link>

    <!-- Neck joint -->
    <joint name="neck_joint" type="revolute">
      <parent>base_link</parent>
      <child>head</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-0.785</lower>
          <upper>0.785</upper>
          <effort>10</effort>
          <velocity>2.0</velocity>
        </limit>
      </axis>
      <pose relative_to="base_link">0 0 0.4 0 0 0</pose>
    </joint>

    <!-- Left Arm -->
    <link name="left_upper_arm">
      <inertial>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.01</iyy>
          <iyz>0.0</iyz>
          <izz>0.005</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.3</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.0 0.0 0.8 1</ambient>
          <diffuse>0.0 0.0 0.8 1</diffuse>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.3</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <joint name="left_shoulder_joint" type="revolute">
      <parent>base_link</parent>
      <child>left_upper_arm</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1.57</lower>
          <upper>1.57</upper>
          <effort>15</effort>
          <velocity>1.5</velocity>
        </limit>
      </axis>
      <pose relative_to="base_link">0.15 0.1 0.3 0 0 0</pose>
    </joint>

    <link name="left_lower_arm">
      <inertial>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.005</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.005</iyy>
          <iyz>0.0</iyz>
          <izz>0.003</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.04</radius>
            <length>0.25</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.0 0.0 0.8 1</ambient>
          <diffuse>0.0 0.0 0.8 1</diffuse>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.04</radius>
            <length>0.25</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <joint name="left_elbow_joint" type="revolute">
      <parent>left_upper_arm</parent>
      <child>left_lower_arm</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1.57</lower>
          <upper>1.57</upper>
          <effort>10</effort>
          <velocity>1.0</velocity>
        </limit>
      </axis>
      <pose relative_to="left_upper_arm">0 0 -0.3 0 0 0</pose>
    </joint>

    <!-- Right Arm -->
    <link name="right_upper_arm">
      <inertial>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.01</iyy>
          <iyz>0.0</iyz>
          <izz>0.005</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.3</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.0 0.0 0.8 1</ambient>
          <diffuse>0.0 0.0 0.8 1</diffuse>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.3</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <joint name="right_shoulder_joint" type="revolute">
      <parent>base_link</parent>
      <child>right_upper_arm</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1.57</lower>
          <upper>1.57</upper>
          <effort>15</effort>
          <velocity>1.5</velocity>
        </limit>
      </axis>
      <pose relative_to="base_link">0.15 -0.1 0.3 0 0 0</pose>
    </joint>

    <link name="right_lower_arm">
      <inertial>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.005</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.005</iyy>
          <iyz>0.0</iyz>
          <izz>0.003</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.04</radius>
            <length>0.25</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.0 0.0 0.8 1</ambient>
          <diffuse>0.0 0.0 0.8 1</diffuse>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.04</radius>
            <length>0.25</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <joint name="right_elbow_joint" type="revolute">
      <parent>right_upper_arm</parent>
      <child>right_lower_arm</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1.57</lower>
          <upper>1.57</upper>
          <effort>10</effort>
          <velocity>1.0</velocity>
        </limit>
      </axis>
      <pose relative_to="right_upper_arm">0 0 -0.3 0 0 0</pose>
    </joint>

    <!-- Left Leg -->
    <link name="left_upper_leg">
      <inertial>
        <mass>3.0</mass>
        <inertia>
          <ixx>0.05</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.05</iyy>
          <iyz>0.0</iyz>
          <izz>0.02</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.08</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.8 0.0 0.0 1</ambient>
          <diffuse>0.8 0.0 0.0 1</diffuse>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.08</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <joint name="left_hip_joint" type="revolute">
      <parent>base_link</parent>
      <child>left_upper_leg</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-0.785</lower>
          <upper>0.785</upper>
          <effort>20</effort>
          <velocity>1.0</velocity>
        </limit>
      </axis>
      <pose relative_to="base_link">-0.1 0.08 0 0 0 0</pose>
    </joint>

    <link name="left_lower_leg">
      <inertial>
        <mass>2.5</mass>
        <inertia>
          <ixx>0.04</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.04</iyy>
          <iyz>0.0</iyz>
          <izz>0.015</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.07</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.8 0.0 0.0 1</ambient>
          <diffuse>0.8 0.0 0.0 1</diffuse>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.07</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <joint name="left_knee_joint" type="revolute">
      <parent>left_upper_leg</parent>
      <child>left_lower_leg</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>0</lower>
          <upper>1.57</upper>
          <effort>20</effort>
          <velocity>1.0</velocity>
        </limit>
      </axis>
      <pose relative_to="left_upper_leg">0 0 -0.4 0 0 0</pose>
    </joint>

    <link name="left_foot">
      <inertial>
        <mass>0.8</mass>
        <inertia>
          <ixx>0.002</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.003</iyy>
          <iyz>0.0</iyz>
          <izz>0.004</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <box>
            <size>0.15 0.08 0.04</size>
          </box>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.3 0.3 0.3 1</diffuse>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <box>
            <size>0.15 0.08 0.04</size>
          </box>
        </geometry>
      </collision>
    </link>

    <joint name="left_ankle_joint" type="revolute">
      <parent>left_lower_leg</parent>
      <child>left_foot</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-0.52</lower>
          <upper>0.52</upper>
          <effort>10</effort>
          <velocity>0.5</velocity>
        </limit>
      </axis>
      <pose relative_to="left_lower_leg">0 0 -0.4 0 0 0</pose>
    </joint>

    <!-- Right Leg -->
    <link name="right_upper_leg">
      <inertial>
        <mass>3.0</mass>
        <inertia>
          <ixx>0.05</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.05</iyy>
          <iyz>0.0</iyz>
          <izz>0.02</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.08</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.8 0.0 0.0 1</ambient>
          <diffuse>0.8 0.0 0.0 1</diffuse>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.08</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <joint name="right_hip_joint" type="revolute">
      <parent>base_link</parent>
      <child>right_upper_leg</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-0.785</lower>
          <upper>0.785</upper>
          <effort>20</effort>
          <velocity>1.0</velocity>
        </limit>
      </axis>
      <pose relative_to="base_link">-0.1 -0.08 0 0 0 0</pose>
    </joint>

    <link name="right_lower_leg">
      <inertial>
        <mass>2.5</mass>
        <inertia>
          <ixx>0.04</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.04</iyy>
          <iyz>0.0</iyz>
          <izz>0.015</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.07</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.8 0.0 0.0 1</ambient>
          <diffuse>0.8 0.0 0.0 1</diffuse>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.07</radius>
            <length>0.4</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <joint name="right_knee_joint" type="revolute">
      <parent>right_upper_leg</parent>
      <child>right_lower_leg</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>0</lower>
          <upper>1.57</upper>
          <effort>20</effort>
          <velocity>1.0</velocity>
        </limit>
      </axis>
      <pose relative_to="right_upper_leg">0 0 -0.4 0 0 0</pose>
    </joint>

    <link name="right_foot">
      <inertial>
        <mass>0.8</mass>
        <inertia>
          <ixx>0.002</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.003</iyy>
          <iyz>0.0</iyz>
          <izz>0.004</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <box>
            <size>0.15 0.08 0.04</size>
          </box>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.3 0.3 0.3 1</diffuse>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <box>
            <size>0.15 0.08 0.04</size>
          </box>
        </geometry>
      </collision>
    </link>

    <joint name="right_ankle_joint" type="revolute">
      <parent>right_lower_leg</parent>
      <child>right_foot</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-0.52</lower>
          <upper>0.52</upper>
          <effort>10</effort>
          <velocity>0.5</velocity>
        </limit>
      </axis>
      <pose relative_to="right_lower_leg">0 0 -0.4 0 0 0</pose>
    </joint>

    <!-- Sensors -->
    <link name="imu_link">
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.0001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0001</iyy>
          <iyz>0</iyz>
          <izz>0.0001</izz>
        </inertia>
      </link>
      <joint name="imu_joint" type="fixed">
        <parent>base_link</parent>
        <child>imu_link</child>
        <pose relative_to="base_link">0 0 0.1 0 0 0</pose>
      </joint>
      <sensor name="imu_sensor" type="imu">
        <always_on>true</always_on>
        <update_rate>100</update_rate>
        <visualize>true</visualize>
      </sensor>
    </link>

    <link name="camera_link">
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.0001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0001</iyy>
          <iyz>0</iyz>
          <izz>0.0001</izz>
        </inertia>
      </link>
      <joint name="camera_joint" type="fixed">
        <parent>head</parent>
        <child>camera_link</child>
        <pose relative_to="head">0.05 0 0.05 0 0 0</pose>
      </joint>
      <sensor name="camera" type="camera">
        <always_on>true</always_on>
        <update_rate>30</update_rate>
        <camera name="head_camera">
          <horizontal_fov>1.3962634</horizontal_fov>
          <image>
            <width>640</width>
            <height>480</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.1</near>
            <far>100</far>
          </clip>
        </camera>
        <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
          <frame_name>camera_link</frame_name>
          <topic_name>camera/image_raw</topic_name>
        </plugin>
      </sensor>
    </link>

    <!-- Gazebo plugins for ROS control -->
    <gazebo>
      <plugin name="gazebo_ros_control" filename="libgazebo_ros2_control.so">
        <parameters>$(find humanoid_description)/config/joint_names.yaml</parameters>
      </plugin>
    </gazebo>

    <!-- Material definitions for rendering -->
    <gazebo reference="base_link">
      <material>Gazebo/Grey</material>
    </gazebo>

    <gazebo reference="head">
      <material>Gazebo/White</material>
    </gazebo>

    <gazebo reference="left_upper_arm">
      <material>Gazebo/Blue</material>
    </gazebo>

    <gazebo reference="right_upper_arm">
      <material>Gazebo/Blue</material>
    </gazebo>

    <gazebo reference="left_lower_arm">
      <material>Gazebo/Blue</material>
    </gazebo>

    <gazebo reference="right_lower_arm">
      <material>Gazebo/Blue</material>
    </gazebo>

    <gazebo reference="left_upper_leg">
      <material>Gazebo/Red</material>
    </gazebo>

    <gazebo reference="right_upper_leg">
      <material>Gazebo/Red</material>
    </gazebo>

    <gazebo reference="left_lower_leg">
      <material>Gazebo/Red</material>
    </gazebo>

    <gazebo reference="right_lower_leg">
      <material>Gazebo/Red</material>
    </gazebo>

    <gazebo reference="left_foot">
      <material>Gazebo/Black</material>
    </gazebo>

    <gazebo reference="right_foot">
      <material>Gazebo/Black</material>
    </gazebo>
  </model>
</sdf>